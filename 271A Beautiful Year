#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main() {
	
	int y;
	cin >> y;
	int a, b, c, d;
	for(y+=1;;y++){
		a = y % 10;
		b = (y % 100) / 10;
		c = (y % 1000) / 100;
		d = y / 1000;
		if (a != b && a != c && a != d && b != c && b != d && c != d) {
			cout << y;
			break;
		}
	}
	

	return 0;
}
